<h1>Blog Posts</h1>
<ul class="list-group">
    {{#each blogs}}
        <li class="list-group-item">
            <h2><a href="/blogpost/{{this.slug}}">{{this.title}}</a></h2>
            <p>{{this.summary}}</p>
        </li>
    {{/each}}
</ul>

